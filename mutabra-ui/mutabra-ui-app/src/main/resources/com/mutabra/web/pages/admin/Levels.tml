<html t:type="layout/default"
	  xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
	  xmlns:p="tapestry:parameter">

<t:grid source="source" row="row" include="code,rating" add="name,actions">
	<p:nameCell>${row:name}</p:nameCell>
	<p:actionsCell>
		<t:eventlink event="edit" zone="entityDialog" context="row">${message:label.edit}</t:eventlink>
	</p:actionsCell>
</t:grid>

<t:admin.leveldialog t:id="entityDialog"/>

</html>
