<ul class="container m-menu ${className}"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">

	<t:block id="defaultItemBody">
		<t:eventlink event="prop:item">${itemTitle}</t:eventlink>
	</t:block>

	<t:loop source="items" value="item">
		<li class="${itemClass}"><t:delegate to="itemBody"/></li>
	</t:loop>
</ul>