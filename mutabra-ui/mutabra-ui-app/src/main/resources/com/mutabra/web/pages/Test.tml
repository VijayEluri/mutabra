<script type="text/javascript">
	j$(function() {
		j$("#accordion").accordion();
		j$("#tags").autocomplete({
			source: ["c++", "java", "php", "coldfusion", "javascript", "asp", "ruby", "python", "c", "scala", "groovy", "haskell", "perl"]
		});
		j$("#datepicker").datepicker({
			inline: true
		});

		j$("input:submit, a, button", "#buttons").button();
		j$("#radio").buttonset();
		j$("#check").button();
		j$("#format").buttonset();
		j$("#button1").button({
			icons: {
				primary: "ui-icon-locked"
			},
			text: false
		});
		j$("#button2").button({
			icons: {
				primary: "ui-icon-locked"
			}
		});
		j$("#button3").button({
			icons: {
				primary: "ui-icon-gear",
				secondary: "ui-icon-triangle-1-s"
			}
		});
		j$("#button4").button({
			icons: {
				primary: "ui-icon-gear",
				secondary: "ui-icon-triangle-1-s"
			},
			text: false
		});

		j$("#progressbar").progressbar({value: 37});

		j$("#slider-range").slider({
			range: true,
			values: [17, 67]
		});

		j$("#tabs").tabs();

		j$("#dialog").dialog({
			autoOpen: false,
			modal: true,
			buttons: {
				Ok: function() {
					j$(this).dialog("close");
				},
				"Cancel": function() {
					j$(this).dialog("close");
				}
			}
		});
		j$('#dialog_link').button();
		j$('#dialog_link').click(function() {
			j$('#dialog').dialog('open');
			return false;
		});
	});
</script>
