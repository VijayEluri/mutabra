<html t:type="layout/default"
	  xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
	  xmlns:p="tapestry:parameter">

<t:carousel source="source" row="row" value="value">
	<t:image.heroimage hero="row"/>
</t:carousel>

<t:menu class="toolbar" items="enter,create,cancel">
	<p:createMenuItem>
		<t:button>
			<t:pagelink page="game/hero/create">${message:button.create.title}</t:pagelink>
		</t:button>
	</p:createMenuItem>
</t:menu>
</html>
